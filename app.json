{
  "expo": {
    "name": "Oparides",
    "slug": "oparides",
    "owner": "orinadeonnn",
    "version": "1.0.0",
    "sdkVersion": "54.0.0",
    "jsEngine": "hermes",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "icon": "./assets/logo/logo.png",
    "splash": {
      "image": "./assets/logo/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#F7F7F7"
    },
    "assetBundlePatterns": [
      "assets/logo/*",
      "assets/images/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.oparides.app"
    },
    "android": {
      "package": "com.oparides.app",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo/logo.png",
        "backgroundColor": "#F7F7F7"
      },
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION"
      ]
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "jsEngine": "hermes",
            "minifyEnabled": true,
            "enableProguardInReleaseBuilds": true,
            "enableR8": true,
            "enableShrinkResources": true,
            "enableSeparateBuildPerCPUArchitecture": true,
            "proguardFiles": [
              "proguard-rules.pro"
            ],
            "gradleProperties": {
              "android.enableR8.fullMode": true,
              "android.enableResourceShrinking": true
            },
            "enableMinifyInReleaseBuilds": true
          }
        }
      ],
      "expo-sqlite"
    ],
    "extra": {
      "eas": {
        "projectId": "a1abf160-4f92-4e6c-9a14-86f0564ccfef"
      }
    }
  }
}
